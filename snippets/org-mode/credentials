# -*- mode: snippet -*-
# name: credentials
# key: cred
# expand-env: ((level (1+ (org-outline-level-visible))))
# --
`(make-string level ?*)` $1
`(make-string level ? )` :PROPERTIES:
`(make-string level ? )` :NAME: $2
`(make-string level ? )` :PASSWORD: $0
`(make-string level ? )` :XDG:SCHEMA: org.gnome.keyring.NetworkPassword
`(make-string level ? )` :END:
`(make-string level ? )` OPENED: `(and (org-insert-time-stamp (current-time) t t) "")`
